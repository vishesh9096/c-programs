#include<conio.h>
#include<stdio.h>

void main()
{
    int a,b,c,i,j,k,d,e;

    for(i=1;i<=98;i++)
    {
        a=i;
        for(j=1;j<=100;j++)
        {
            b=j;
            for(k=1;k<=100;k++)
            {


                c=k;
                d=(a*a)+(b*b);
                e=c*c;
                if(e==d)
                {
                    printf("\ntriplet is %d %d %d",a,b,c);
                }
            }

        }

    }
    getch();



}
